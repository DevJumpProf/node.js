<div class="card border-primary">
    <div class="card-header bg-primary text-white">Edit usuario <%= old.id %>
    </div>
    <div class="card-body text-primary">
        <form action="/editUser/<%= old.id %>?_method=put " method="POST" enctype="multipart/form-data">
            <div class="mb-3">
                <label for="name" class="form-label">Nombre completo</label>
                <input id="name" name="name" type="text" class="form-control"
                    aria-describedby="emailHelp" value="<%= old.name %>">
            </div>
            <div class="mb-3">
                <label for="user" class="form-label">Cuenta de usuario</label>
                <input id="user" name="user" type="text" class="form-control" value="<%= old.user %>">
            </div>
            <div class="mb-3">
                <label for="avatar" class="form-label">Avatar</label>
                <input id="avatar" name="avatar" type="file" class="form-control">
            </div>
            <div class="mb-3">
                <label for="user" class="form-label">email</label>
                <input id="email" name="email" type="email" class="form-control"
                    value="<%= old.email %>">
            </div>
    </div>
    <div class="card-footer bg-transparent border-primary">
        <a href="/" type="button" class="btn btn-secondary">Cancelar</a>
        <button type="submit" class="btn btn-primary">Editar</button>
    </div>

    
   



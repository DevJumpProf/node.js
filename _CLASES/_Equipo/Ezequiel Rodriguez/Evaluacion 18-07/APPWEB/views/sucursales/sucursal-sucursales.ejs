<%- include('../partials/head-header') %>

    <section>
        <div class="menu-titulo">
            <h2>Nuestras sucursales</h2>
            <br>
            <h3><%= title %> </h3>
        </div>
            <div class="sucursales">
                <ul>
                    <li> <%= sucursales.direccion %> </li>
                    <li> <%= sucursales.telefono %> </li>
                </ul>
            </div>
            <div>
                <ul>
                <% auto.forEach(autos=> { %>
                    
                    <li> <%= autos.marca %> </li>
                    <li> <%= autos.modelo %> </li>
                    <li> <%= autos.anio %> </li>
                    <li> <%= autos.color %> </li>

                    <hr>

                <% }); %>
                <%let total = auto.length %>
                    <li>Cantidad total de autos: <%= total %>  </li>
                </ul>
            </div>
    </section>
    
    <%- include('../partials/footer') %>
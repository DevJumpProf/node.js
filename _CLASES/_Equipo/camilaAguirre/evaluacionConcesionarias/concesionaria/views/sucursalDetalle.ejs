<h3> <%= direccionSucursal%></h3>
<h3> <%= telefonoSucursal %></h3>
<h3><%= cantidad %> </h3>
<div>
    <% autosArray.forEach(auto => { %>
     
        <h4>
            <%= auto.marca %> 
        </h4>
        <h4>
            <%= auto.modelo %> 
        </h4>
        <h4>
            <%= auto.anio %> 
        </h4>
        <h4>
            <%= auto.color %> 
